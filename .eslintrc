{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true,
    },
    "extends": ["eslint:recommended","plugin:react/recommended"],
    "parserOptions": {
        "ecmaVersion": 8,
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true,
            "classes": true,
        },
        "sourceType": "module"
    },
    "parser": "babel-eslint",
    "plugins": [
        "babel",
        "react",
        "import"
    ],
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-unused-vars": [
            "warn"
        ],
        "no-undef": [
            "warn"
        ],
        "no-console": [
            "warn"
        ]
    }
}
